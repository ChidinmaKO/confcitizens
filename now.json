{
  "version": 2,
  "name": "confcitizens",
  "alias": "confcitizens.com",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next",
      "config": { "newPipeline": true }
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index" },
    { "src": "/(.*)", "dest": "/detail?username=$1" }
  ]
}
