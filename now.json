{
  "version": 2,
  "name": "confcitizens",
  "alias": "confcitizens.com",
  "regions": ["all"],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index" },
    {
      "src": "^/(?!_next/|favicon[.]ico)(?<username>.+)$",
      "dest": "/detail?username=$username"
    }
  ]
}
